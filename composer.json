{
  "name": "fond-of-spryker/order-confirmation-bcc",
  "description": "Plugin for sending orderconfirmation in bcc",
  "license": "MIT",
  "authors": [
    {
      "name": "Pascal Fischer",
      "email": "pascal.fischer@fondof.de"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1",
    "spryker/oms": "^10.3.0",
    "spryker/mail": "^4.1.1"
  },
  "require-dev": {
    "spryker/code-sniffer": "^0.14.7",
    "codeception/codeception": "^2.5",
    "mikey179/vfsStream": "^1.6",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.1",
    "php-coveralls/php-coveralls": "^2.0",
    "spryker/kernel": "^3.37.5",
    "spryker/application": "^3.19.0"
  },
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "tests/Generated/"
    }
  }
}
